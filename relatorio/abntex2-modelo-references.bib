@article{Anderl1996a,
abstract = {Modelling with constraints is a modern approach to product modelling. Engineering knowledge is associated with geometry and topology in the product model. Together with the technique of feature based modelling, modelling with constraints has widely affected the development of new CAD-systems. The architecture of so called parametric CAD-systems reflects the impact of these new technologies. Besides the new hybrid modellers of geometry and topology, the sketcher and the constraint solver have become key components in a parametric CAD-system. The sketcher may be regarded as a designer interface for modelling with constraints. Moreover rule-based methods of automatic constraint detection are applied in sketchers. Constraint solvers evaluate the network of constraints formulated by the sketcher as a set of equations or predicates using numeric or symbolic algorithms or rule based approaches. The article closes with a summary of the advantages and risks of designing with constraints, showing typical application fields of this modelling technique and discussing some open issues.},
author = {Anderl, Reiner and Mendgen, Ralf},
doi = {10.1016/0010-4485(95)00023-2},
file = {:C$\backslash$:/Users/ffrib/arquivos de trabalho/MCTI- trabalhos/DISSERTA{\c{C}}{\~{A}}O/copia artigos/Anderl, Mendgen - Modelling with constraints Theoretical foundation and application - 1996 - CAD Computer Aided Design.pdf:pdf},
issn = {00104485},
journal = {CAD Computer Aided Design},
keywords = {Conceptual design,Constraint detection,Constraint solving},
number = {3},
pages = {155--168},
title = {{Modelling with constraints: Theoretical foundation and application}},
volume = {28},
year = {1996}
}
@article{Granadeiro2013a,
abstract = {Generative design systems coupled with objective functions can be efficiently explored through the use of stochastic optimization algorithms, such as genetic algorithms. The first step in implementing genetic algorithms is to define a representation, that is, the data structure representative of the genotype space and its mathematical relation to the data of the phenotype space - the variables of the real problem. This can be a hard task, particularly if the design system contains dependency between variables. This paper presents a general representation, which enables the use of standard variation operators, allows defining both continuous and discrete variables from a single type of gene and is easily adaptable to different problems, with a larger or smaller number of variables. This representation was created to solve the representation problem in the design system for Frank Lloyd Wright's prairie houses, a shape grammar that was converted into a parametric design system. {\textcopyright} 2013 Elsevier B.V.},
author = {Granadeiro, Vasco and Pina, Luis and Duarte, Jos{\'{e}} P. and Correia, Jo{\~{a}}o R. and Leal, V{\'{i}}tor M S},
doi = {10.1016/j.autcon.2013.05.012},
file = {:C$\backslash$:/Users/ffrib/arquivos de trabalho/MCTI- trabalhos/DISSERTA{\c{C}}{\~{A}}O/copia artigos/Granadeiro et al. - A general indirect representation for optimization of generative design systems by genetic algorithms Application to.pdf:pdf},
issn = {09265805},
journal = {Automation in Construction},
keywords = {Generative design system,Genetic algorithm,Optimization,Parametric design,Representation,Shape grammars},
month = {nov},
pages = {374--382},
