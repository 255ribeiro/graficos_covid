issn = {09265805},
journal = {Automation in Construction},
keywords = {BIM (Building Information Modelling),Internet of Things (IoT),Multi-dimensional,Real-time,Visibility and traceability},
pages = {59--70},
publisher = {Elsevier B.V.},
title = {{Prefabricated construction enabled by the Internet-of-Things}},
url = {http://dx.doi.org/10.1016/j.autcon.2017.01.006},
volume = {76},
year = {2017}
}
