issn = {09659978},
journal = {Advances in Engineering Software},
keywords = {Adaptive system,Dam,Genetic algorithms,Linear regression,Modeling,Monitoring},
month = {nov},
pages = {182--190},
title = {{Adaptive system for dam behavior modeling based on linear regression and genetic algorithms}},
