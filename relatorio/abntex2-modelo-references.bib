journal = {Procedia Engineering},
keywords = {Adoption Rate,Barriers,Building Information Modeling,Return on Investment},
pages = {1227--1234},
publisher = {Elsevier B.V.},
title = {{Analysis of the Adoption Rate of Building Information Modeling [BIM] and its Return on Investment [ROI]}},
url = {http://dx.doi.org/10.1016/j.proeng.2017.02.144},
volume = {172},
year = {2017}
}
