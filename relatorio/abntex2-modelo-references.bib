issn = {18770509},
journal = {Procedia Computer Science},
keywords = {Controlling generative process,Creative process,Generative art},
month = {jan},
pages = {43--52},
title = {{Controlling generative processes of generative art}},
url = {http://ac.els-cdn.com/S1877050912007193/1-s2.0-S1877050912007193-main.pdf?{\_}tid=619380d2-499c-11e4-806a-00000aacb35f{\&}acdnat=1412189853{\_}2f5a4c7cf8710836199c714769aeff6b{\%}5Cnhttp://www.sciencedirect.com/science/article/pii/S1877050912007193{\#}},
